{
    "manifest_version": 3,
    "background": {
        "service_worker": "background/main.js"
    },
    "action": {
        "default_popup": "popup/index.html"
    },
    "icons": {
        "16": "assets/icon-16.png",
        "48": "assets/icon-48.png",
        "128": "assets/icon-128.png"
    },
    "web_accessible_resources": {
        "resources": ["assets/*"],
        "matches": ["<all_urls>"]
    }
}